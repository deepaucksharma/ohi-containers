# New Relic Configuration
NEW_RELIC_LICENSE_KEY=your-license-key-here
NEW_RELIC_API_URL=https://infra-api.newrelic.com/v1

# MySQL Integration Configuration
MYSQL_HOST=mysql
MYSQL_PORT=3306
MYSQL_ROOT_PASSWORD=root
MYSQL_DATABASE=test
MYSQL_USER=newrelic
MYSQL_PASSWORD=test_password

# MySQL Integration Metrics Configuration
SLOW_QUERIES=1
EXTENDED_METRICS=1
EXTENDED_INNODB_METRICS=1
EXTENDED_INNODB_METRICS_AS_SAMPLES=1
SLAVE_METRICS=1
PROCESSLIST_METRICS=1
TABLE_SCHEMA_METRICS=1
TABLESPACE_METRICS=1

# PostgreSQL Integration Configuration
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=postgres

# PostgreSQL Integration Metrics Configuration
EXTENDED_METRICS=1
DEFAULT_METRICS_HIGHLIGHTS=1
ENABLE_QUERY_MONITORING=1
METRICS_FROM_ALL_DATABASES=1
DB_LOCK_METRICS=1
DB_BLOAT_METRICS=1
DB_VACUUM_METRICS=1
PG_STAT_STATEMENTS=1
PG_STAT_ACTIVITY=1

# Infrastructure Agent Configuration
NRIA_VERBOSE=1
NRIA_DISPLAY_NAME=My Infrastructure
NRIA_METRICS_SYSTEM_SAMPLE_RATE=15
NRIA_METRICS_STORAGE_SAMPLE_RATE=15
NRIA_METRICS_NETWORK_SAMPLE_RATE=15
NRIA_METRICS_PROCESS_SAMPLE_RATE=15

# Testing Configuration
TEST_RUN_ID=test-run-1
TEST_OUTPUT_DIR=./testing/output
